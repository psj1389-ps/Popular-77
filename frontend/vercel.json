{
  "version": 2,
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/pdf-doc/(.*)",
      "destination": "https://pdf-doc.onrender.com/$1"
    },
    {
      "source": "/api/pdf-jpg/:path*",
      "destination": "https://pdf-jpg-jh3s.onrender.com/:path*"
    },
    {
      "source": "/api/pdf-png/:path*",
      "destination": "https://pdf-png-service.onrender.com/:path*"
    },
    {
      "source": "/api/pdf-xls/:path*",
      "destination": "https://pdf-xls-service.onrender.com/:path*"
    },
    {
      "source": "/api/pdf-ai/:path*",
      "destination": "https://pdf-ai-service.onrender.com/:path*"
    },
    {
      "source": "/api/pdf-bmp/:path*",
      "destination": "https://pdf-bmp-service.onrender.com/:path*"
    },
    {
      "source": "/api/pdf-gif/:path*",
      "destination": "https://pdf-gif-service.onrender.com/:path*"
    },
    {
      "source": "/api/pdf-pptx/:path*",
      "destination": "https://pdf-pptx-service.onrender.com/:path*"
    },
    {
      "source": "/api/pdf-svg/:path*",
      "destination": "https://pdf-svg-service.onrender.com/:path*"
    },
    {
      "source": "/api/pdf-tiff/:path*",
      "destination": "https://pdf-tiff-service.onrender.com/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}