{
  "version": 2,
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/pdf-doc/(.*)",
      "destination": "https://pdf-doc-306w.onrender.com/$1"
    },
    {
      "source": "/api/pdf-bmp/(.*)",
      "destination": "https://pdf-bmp.onrender.com/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}